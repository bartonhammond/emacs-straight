    ],
    ];
    /**
     * Click on Add New Registrant Link.
     *
     */
    function clickAddNewRegistrantLink() {
        //link
        $this->click(self::$_selectors['addNewRegistrant']);
    }

    /**
     * Click on 3billing Link.
     *
     */
    function clickBillingLink3() {
        //link
        $this->click(self::$_selectors['billing3']);
    }

    /**
     * Click on Bwh Music Course Link.
     *
     */
    function clickBwhMusicCourseLink() {
        //link
        $this->click(self::$_selectors['bwhMusicCourse']);
    }

    /**
     * Click on 2confirm Link.
     *
     */
    function clickConfirmLink2() {
        //link
        $this->click(self::$_selectors['confirm2']);
    }

    /**
     * Click on Continue To Cart Button.
     *
     */
    function clickContinueToCartButton() {
        //button
        $this->click(self::$_selectors['continueToCart']);
    }

    /**
     * Click on Course Catalog Link.
     *
     */
    function clickCourseCatalogLink() {
        //link
        $this->click(self::$_selectors['courseCatalog']);
    }

    /**
     * Click on Find Descriptions And Details On All Workshops Offered In This Program Below. Link.
     *
     */
    function clickFindDescriptionsAndDetailsOnAllLink() {
        //link
        $this->click(self::$_selectors['findDescriptionsAndDetailsOnAll']);
    }

    /**
     * Click on Home Link.
