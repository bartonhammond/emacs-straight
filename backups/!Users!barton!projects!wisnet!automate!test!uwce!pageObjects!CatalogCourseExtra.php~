class CatalogCourse extends \Automate\Page {










     protected $pageLoadedText = "Find descriptions and details on all workshops offered in this program below";

     protected $pageUrl = "/uwce-staging/www/fond-du-lac/catalog/bwh-music-course/407";











   private static $_selectors = [
    "addNewRegistrant" =>
    [
       "css" => "a.ajax-action.btn.btn-default"
    ],

    "billing3" =>
    [
       "css" => "#registrantsform div:nth-of-type(1) span:nth-of-type(2) a:nth-of-type(3)"
    ],

    "bwhMusicCourse" =>
    [
       "css" => "a[href='/uwce-staging/www/fond-du-lac/catalog/bwh-music-course']"
    ],

